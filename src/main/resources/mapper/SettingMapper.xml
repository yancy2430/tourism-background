<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tdeado.tourism.dao.SettingMapper">

    <update id="upSetting" parameterType="Map">
        UPDATE `tb_setting` SET `setting_value` = #{settingValue} WHERE `setting_name` = #{settingName}
    </update>

</mapper>